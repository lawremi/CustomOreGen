              <Setting name='OreRadiusMult' avg=':= _default_ ' range=':= _default_ ' type='normal' />
              <Veins name='neotechTinHintVeins' branchType='Bezier'  inherits='PresetHintVeins' drawWireframe=':= drawWireframes' wireframeColor='0x60D1EDF1' drawBoundBox='false' boundBoxColor='0x60D1EDF1'>
