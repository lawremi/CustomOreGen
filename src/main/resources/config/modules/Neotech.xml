        <OptionNumeric name='neotechTinFreq' default='1'  min='0' max='5' displayState=':= if(?enableNeotech, if(?advOptions, "shown", "hidden"), "hidden")' displayGroup='groupNeotech'>
          <Description> Frequency multiplier for Neotech Tin distributions </Description>
          <DisplayName>Neotech Tin Freq.</DisplayName>
        </OptionNumeric>
        <OptionNumeric name='neotechTinSize' default='1'  min='0' max='5' displayState=':= if(?enableNeotech, if(?advOptions, "shown", "hidden"), "hidden")' displayGroup='groupNeotech'>
          <Description> Size multiplier for Neotech Tin distributions </Description>
          <DisplayName>Neotech Tin Size</DisplayName>
        </OptionNumeric>
