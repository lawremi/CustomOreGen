<!-- =================================================================
     Custom Ore Generation "Neotech" Module: This configuration covers
     copper, tin, lead, and silver.
     ================================================================= -->


<!-- A mod for ore processing. Configuration by leagris. -->




<!-- Is the "Neotech" mod on the system?  Let's find out! -->
<IfModInstalled name="neotech">

  <!-- Starting Configuration for Custom Ore Generation. -->
  <ConfigSection>





    <!-- Setup Screen Configuration -->
    <ConfigSection>
      <OptionDisplayGroup name='groupNeotech' displayName='Neotech' displayState='shown'>
        <Description>
          Distribution options for Neotech Ores.
        </Description>
      </OptionDisplayGroup>
      <OptionChoice name='enableNeotech' displayName='Handle Neotech Setup?' default='true' displayState='shown_dynamic' displayGroup='groupNeotech'>
        <Description> Should Custom Ore Generation handle Neotech ore generation? </Description>
        <Choice value=':= ?true' displayValue='Yes' description='Use Custom Ore Generation to handle Neotech ores.'/>
        <Choice value=':= ?false' displayValue='No' description='Neotech ores will be handled by Minecraft directly.'/>
      </OptionChoice>
      <OptionChoice name='cleanUpNeotech' displayName='Use Neotech Cleanup?' default='true' displayState=':= if(?enableNeotech, "shown", "hidden")' displayGroup='groupNeotech'>
        <Description> Should Custom Ore Generation use the Substitution Pass to remove all instances of Neotech ore from the world?  If the mod's oregen can be turned off in its configuration, then it's recommended to do so, as the substitution pass can slow the game significantly.  If this option is disabled without disabling the mod's own ore generation, you'll end up with two oregens working at once, flooding the world with ore.  Enabled by default to ensure the ores are completely removed. </Description>
        <Choice value=':= ?true' displayValue='Yes' description='Use the substitution pass to clean up Neotech ores.'/>
        <Choice value=':= ?false' displayValue='No' description='Neotech ores do not need to be cleaned up by a substitution pass.'/>
      </OptionChoice>

      <!-- Copper Configuration UI Starting -->
      <ConfigSection>
        <OptionChoice name='neotechCopperDist'  displayState=':= if(?enableNeotech, "shown", "hidden")' displayGroup='groupNeotech'>
          <Description> Controls how Copper is generated </Description>
          <DisplayName>Neotech Copper</DisplayName>
          <IfCondition condition=':= (?blockExists("neotech:orecopper")) '>

          <Choice value='LayeredVeins' displayValue='Layered Veins'>
            <Description>
              Small, fairly rare motherlodes with 2-4 horizontal veins each.
            </Description>
          </Choice>
          </IfCondition>

          <IfCondition condition=':= (?blockExists("neotech:orecopper")) '>

          <Choice value='StrategicClouds' displayValue='Strategic Clouds'>
            <Description>
              Large irregular clouds filled lightly with ore.
            </Description>
          </Choice>
          </IfCondition>

          <IfCondition condition=':= (?blockExists("neotech:orecopper")) '>

          <Choice value='Vanilla' displayValue='Vanilla'>
            <Description>
              Simulates Vanilla Minecraft.
            </Description>
          </Choice>
          </IfCondition>

          <Choice value='none' displayValue='None' description='Copper is not generated in the world.'/>
        </OptionChoice>
        <OptionNumeric name='neotechCopperFreq' default='1'  min='0' max='5' displayState=':= if(?enableNeotech, if(?advOptions, "shown", "hidden"), "hidden")' displayGroup='groupNeotech'>
          <Description> Frequency multiplier for Neotech Copper distributions </Description>
          <DisplayName>Neotech Copper Freq.</DisplayName>
        </OptionNumeric>
        <OptionNumeric name='neotechCopperSize' default='1'  min='0' max='5' displayState=':= if(?enableNeotech, if(?advOptions, "shown", "hidden"), "hidden")' displayGroup='groupNeotech'>
          <Description> Size multiplier for Neotech Copper distributions </Description>
          <DisplayName>Neotech Copper Size</DisplayName>
        </OptionNumeric>
      </ConfigSection>
      <!-- Copper Configuration UI Complete -->


      <!-- Tin Configuration UI Starting -->
      <ConfigSection>
        <OptionChoice name='neotechTinDist'  displayState=':= if(?enableNeotech, "shown", "hidden")' displayGroup='groupNeotech'>
          <Description> Controls how Tin is generated </Description>
          <DisplayName>Neotech Tin</DisplayName>
          <IfCondition condition=':= (?blockExists("neotech:oretin")) '>

          <Choice value='LayeredVeins' displayValue='Layered Veins'>
            <Description>
              Small, fairly rare motherlodes with 2-4 horizontal veins each.
