              <Setting name='Height' avg=':= 55 ' range=':= 30 ' type='uniform' scaleTo='base' />
              <Setting name='ParentRangeLimit' avg=':= _default_ ' range=':= _default_ ' type='normal' />
            </StandardGen>
          </IfCondition>
        </ConfigSection>
