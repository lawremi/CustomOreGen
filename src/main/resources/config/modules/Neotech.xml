        <OptionNumeric name='neotechSilverFreq' default='1'  min='0' max='5' displayState=':= if(?enableNeotech, if(?advOptions, "shown", "hidden"), "hidden")' displayGroup='groupNeotech'>
          <Description> Frequency multiplier for Neotech Silver distributions </Description>
          <DisplayName>Neotech Silver Freq.</DisplayName>
        </OptionNumeric>
